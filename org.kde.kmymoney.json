{
    "id": "org.kde.kmymoney",
    "default-branch": "stable",
    "runtime": "org.kde.Platform",
    "runtime-version": "5.15-23.08",
    "sdk": "org.kde.Sdk",
    "base": "io.qt.qtwebengine.BaseApp",
    "base-version": "5.15-23.08",
    "command": "kmymoney",
    "rename-icon": "kmymoney",
    "copy-icon": true,
    "cleanup": [
        "/include",
        "/lib/pkgconfig",
        "/lib/*.a",
        "/lib/*.la",
        "/share/man"
    ],
    "cleanup-commands": [
        "/app/cleanup-BaseApp.sh"
    ],
    "finish-args": [
        "--share=ipc",
        "--share=network",
        "--socket=wayland",
        "--socket=fallback-x11",
        "--socket=pulseaudio",
        "--device=dri",
        "--system-talk-name=org.freedesktop.UDisks2"
    ],
    "modules": [
        {
            "name": "alkimia",
            "buildsystem": "cmake-ninja",
            "sources": [
                {
                    "type": "archive",
                    "url": "https://download.kde.org/stable/alkimia/8.1.1/alkimia-8.1.1.tar.xz",
                    "sha256": "957adc63c0be54dd433e3268a37323be5456e5ab84ece27f73a1616a92db86d5"
                }
            ]
        },
        {
            "name": "sigc++",
            "sources": [
                {
                    "sha256": "235a40bec7346c7b82b6a8caae0456353dc06e71f14bc414bcc858af1838719a",
                    "type": "archive",
                    "url": "https://download.gnome.org/sources/libsigc++/2.10/libsigc++-2.10.8.tar.xz",
                    "x-checker-data": {
                        "type": "gnome",
                        "name": "libsigc++",
                        "versions": {
                            "<": "2.11"
                        }
                    }
                }
            ],
            "buildsystem": "meson",
            "config-opts": [
                "-Dbuild-examples=false"
            ],
            "cleanup": [
                "*.la",
                "/include/sigc++-2.0",
                "/lib/sigc++-2.0"
            ]
        },
        {
            "name": "glibmm",
            "sources": [
                {
                    "sha256": "fe02c1e5f5825940d82b56b6ec31a12c06c05c1583cfe62f934d0763e1e542b3",
                    "type": "archive",
                    "url": "https://download.gnome.org/sources/glibmm/2.66/glibmm-2.66.7.tar.xz",
                    "x-checker-data": {
                        "type": "gnome",
                        "name": "glibmm",
                        "versions": {
                            "<": "2.67"
                        }
                    }
                }
            ],
            "buildsystem": "meson",
            "config-opts": [
                "-Dbuild-examples=false"
            ],
            "cleanup": [
                "*.la",
                "/include/glibmm-2.4",
                "/include/giomm-2.4",
                "/lib/glibmm-2.4",
                "/lib/giomm-2.4",
                "/lib/libglibmm_generate_extra_defs*"
            ]
        },
        {
            "name": "mm-common",
            "sources": [
                {
                    "type": "archive",
                    "url": "https://download.gnome.org/sources/mm-common/1.0/mm-common-1.0.6.tar.xz",
                    "sha256": "b55c46037dbcdabc5cee3b389ea11cc3910adb68ebe883e9477847aa660862e7",
                    "x-checker-data": {
                        "type": "gnome",
                        "name": "mm-common",
                        "stable-only": true
                    }
                }
            ]
        },
        {
            "name": "libxml++-2.6",
            "buildsystem": "meson",
            "config-opts": [
                "-Dbuild-examples=false",
                "-Dbuild-tests=false"
            ],
            "cleanup": [
                "/lib/libxml++-2.6"
            ],
            "sources": [
                {
                    "type": "archive",
                    "url": "https://download.gnome.org/sources/libxml++/2.91/libxml++-2.91.3.tar.xz",
                    "sha256": "f9f910cacd1d07c71679eeea36ae018189b72b4b7dd11511a76562377ed4d06a",
                    "x-checker-data": {
                        "type": "gnome",
                        "name": "libxml++",
                        "//": "libxml++ 3+ is not API compatible",
                        "versions": {
                            "<": "2.92.0"
                        },
                        "stable-only": true
                    }
                }
            ]
        },
        {
            "name": "libofx",
            "buildsystem": "cmake-ninja",
            "config-opts": [
                "-DBUILD_SHARED_LIBS=ON"
            ],
            "sources": [
                {
                    "type": "archive",
                    "url": "https://github.com/libofx/libofx/releases/download/0.10.9/libofx-0.10.9.tar.gz",
                    "sha256": "1ca89ff7d681c9edad312172ac464231a8de686e653213612f9417492cef0d37"
                }
            ],
            "modules": [
                {
                    "name": "OpenSP",
                    "buildsystem": "autotools",
                    "rm-configure": true,
                    "config-opts": [
                        "--disable-static",
                        "--disable-doc-build"
                    ],
                    "sources": [
                        {
                            "type": "archive",
                            "url": "http://sourceforge.net/projects/openjade/files/opensp/1.5.2/OpenSP-1.5.2.tar.gz",
                            "sha256": "57f4898498a368918b0d49c826aa434bb5b703d2c3b169beb348016ab25617ce"
                        },
                        {
                            "type": "shell",
                            "commands": [
                                "cp /usr/share/gnu-config/config.sub .",
                                "cp /usr/share/gnu-config/config.guess ."
                            ]
                        },
                        {
                            "type": "script",
                            "dest-filename": "autogen.sh",
                            "commands": [
                                "autoreconf -vfi"
                            ]
                        }
                    ]
                }
            ]
        },
        {
            "name": "akonadi",
            "config-opts": [
                "-DBUILD_TESTING=OFF",
                "-DBUILD_TOOLS=OFF",
                "-DCMAKE_BUILD_TYPE=RelWithDebInfo",
                "-DDATABASE_BACKEND=SQLITE"
            ],
            "buildsystem": "cmake-ninja",
            "sources": [
                {
                    "type": "archive",
                    "url": "https://download.kde.org/stable/release-service/24.08.3/src/akonadi-24.08.3.tar.xz",
                    "sha256": "953e1c56b8c7756826ee1eeca891ea3a194667b432c1f5f6c4ea00c6a0fc466b",
                    "x-checker-data": {
                        "type": "anitya",
                        "project-id": 8763,
                        "stable-only": true,
                        "versions": {
                            "<": "24.08.5"
                        },
                        "url-template": "https://download.kde.org/stable/release-service/$version/src/akonadi-$version.tar.xz"
                    }
                }
            ],
            "modules": [
                {
                    "name": "sqlite3",
                    "config-opts": [
                        "--enable-threadsafe",
                        "--enable-threads-override-locks"
                    ],
                    "build-options": {
                        "cflags": "-DSQLITE_ENABLE_UNLOCK_NOTIFY=1"
                    },
                    "sources": [
                        {
                            "type": "archive",
                            "url": "https://www.sqlite.org/2023/sqlite-autoconf-3440200.tar.gz",
                            "sha256": "1c6719a148bc41cf0f2bbbe3926d7ce3f5ca09d878f1246fcc20767b175bb407",
                            "x-checker-data": {
                                "type": "anitya",
                                "project-id": 4877,
                                "stable-only": true,
                                "url-template": "https://www.sqlite.org/2023/sqlite-autoconf-${major}${minor}0${patch}00.tar.gz"
                            }
                        }
                    ],
                    "cleanup": [
                        "/bin/sqlite3",
                        "*.a"
                    ]
                }
            ]
        },
        {
            "name": "kidentitymanagement",
            "buildsystem": "cmake-ninja",
            "config-opts": [
                "-DBUILD_TESTING=OFF"
            ],
            "sources": [
                {
                    "type": "archive",
                    "url": "https://download.kde.org/stable/release-service/23.08.4/src/kidentitymanagement-23.08.4.tar.xz",
                    "sha256": "92736693685342c724034dd5675df858678f73fc1ed04189a7841e9b2237e89b",
                    "x-checker-data": {
                        "type": "anitya",
                        "project-id": 8763,
                        "stable-only": true,
                        "versions": {
                            "<": "23.08.5"
                        },
                        "url-template": "https://download.kde.org/stable/release-service/$version/src/kidentitymanagement-$version.tar.xz"
                    }
                }
            ],
            "modules": [
                {
                    "name": "kpimtextedit",
                    "config-opts": [
                        "-DBUILD_TESTING=OFF"
                    ],
                    "buildsystem": "cmake-ninja",
                    "sources": [
                        {
                            "type": "archive",
                            "url": "https://download.kde.org/stable/release-service/23.08.4/src/kpimtextedit-23.08.4.tar.xz",
                            "sha256": "da55922ce882c2fed5efca00177fddd6fe503547b8942b22181961e871710de5",
                            "x-checker-data": {
                                "type": "anitya",
                                "project-id": 8763,
                                "stable-only": true,
                                "versions": {
                                    "<": "23.08.5"
                                },
                                "url-template": "https://download.kde.org/stable/release-service/$version/src/kpimtextedit-$version.tar.xz"
                            }
                        }
                    ]
                }
            ]
        },
        {
            "name": "kchart",
            "buildsystem": "cmake-ninja",
            "sources": [
                {
                    "type": "archive",
                    "url": "https://download.kde.org/stable/kdiagram/2.8.0/kdiagram-2.8.0.tar.xz",
                    "sha256": "579dad3bd1ea44b5a20c0f133ebf47622e38960f9c7c8b3a316be30a369f431f",
                    "x-checker-data": {
                        "type": "anitya",
                        "project-id": 17734,
                        "stable-only": true,
                        "versions": {
                            "<": "2.8.1"
                        },
                        "url-template": "https://download.kde.org/stable/kdiagram/$version/kdiagram-$version.tar.xz"
                    }
                }
            ]
        },
        {
            "name": "aqbanking",
            "no-parallel-make": true,
            "sources": [
                {
                    "type": "archive",
                    "url": "https://www.aquamaniac.de/rdm/attachments/download/499/aqbanking-6.5.4.tar.gz",
                    "sha256": "0d16ceae76f0718e466638f4547a8b14927f1d8d98322079cd6481adde30ac99"
                }
            ],
            "modules": [
                {
                    "name": "xmlsec",
                    "sources": [
                        {
                            "type": "archive",
                            "url": "https://github.com/lsh123/xmlsec/archive/refs/tags/xmlsec-1_2_32.tar.gz",
                            "sha256": "e33e551fb9f6da5576b1a55ce962048adf337332c27e6be9dba04d360b6a5584"
                        }
                    ]
                },
                {
                    "name": "libpcsclite",
                    "config-opts": [
                        "--disable-libsystemd",
                        "--disable-serial",
                        "--disable-usb",
                        "--disable-libudev",
                        "--with-systemdsystemunitdir=no"
                    ],
                    "sources": [
                        {
                            "type": "archive",
                            "url": "https://pcsclite.apdu.fr/files/pcsc-lite-1.8.24.tar.bz2",
                            "sha256": "b81864fa6a5ec776639c02ae89998955f7702a8d10e8b8f70023c5a599d97568"
                        }
                    ]
                },
                {
                    "name": "gwenhywfar",
                    "config-opts": [
                        "--with-guis='qt5'"
                    ],
                    "sources": [
                        {
                            "type": "archive",
                            "url": "https://www.aquamaniac.de/rdm/attachments/download/465/gwenhywfar-5.10.1.tar.gz",
                            "sha256": "a2f60a9dde5da27e57e0e5ef5f8931f495c1d541ad90a841e2b6231565547160"
                        }
                    ]
                },
                {
                    "name": "libchipcard",
                    "sources": [
                        {
                            "type": "archive",
                            "url": "https://aquamaniac.de/rdm/attachments/download/382/libchipcard-5.1.6.tar.gz",
                            "sha256": "6c07f527417f7562074f99012da4d11eb4dbafd33f49e66b442ccd6e3b8cfd20"
                        }
                    ]
                }
            ]
        },
        {
            "name": "sqlcipher",
            "cleanup": [
                "/bin"
            ],
            "config-opts": [
                "--enable-tempstore=yes",
                "--disable-tcl"
            ],
            "build-options": {
                "cflags": "-DSQLITE_HAS_CODEC",
                "ldflags": "-lcrypto"
            },
            "sources": [
                {
                    "type": "git",
                    "url": "https://github.com/sqlcipher/sqlcipher",
                    "tag": "v4.4.0",
                    "commit": "4a81bea61e1da6fec222d713852830f1fd01aed2",
                    "disable-fsckobjects": true
                }
            ],
            "modules": [
                {
                    "name": "tcl",
                    "subdir": "unix",
                    "build-options": {
                        "no-debuginfo": true
                    },
                    "cleanup": [
                        "*"
                    ],
                    "sources": [
                        {
                            "type": "archive",
                            "url": "https://downloads.sourceforge.net/project/tcl/Tcl/8.6.10/tcl8.6.10-src.tar.gz",
                            "sha256": "5196dbf6638e3df8d5c87b5815c8c2b758496eb6f0e41446596c9a4e638d87ed"
                        }
                    ]
                }
            ]
        },
        {
            "name": "kmymoney",
            "buildsystem": "cmake-ninja",
            "config-opts": [
                "-DENABLE_WEBENGINE=ON"
            ],
            "cleanup": [
                "/share/icons/hicolor/1024x1024"
            ],
            "builddir": true,
            "sources": [
                {
                    "type": "archive",
                    "url": "https://download.kde.org/stable/kmymoney/5.1.3/src/kmymoney-5.1.3.tar.xz",
                    "sha256": "3938b8078b7391ba32e12bb4239762fae134683a0c2ec1a75105c302ca3e5e3f",
                    "x-checker-data": {
                        "type": "anitya",
                        "project-id": 8917,
                        "stable-only": true,
                        "url-template": "https://download.kde.org/stable/kmymoney/$version/src/kmymoney-$version.tar.xz"
                    }
                }
            ]
        }
    ]
}
